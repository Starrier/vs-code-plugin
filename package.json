{
    "name": "starrier",
    "displayName": "Starrier",
    "description": "Custom for Starrier",
    "version": "0.0.1",
    "publisher": "Starrier",
    "repository": {
        "type": "git",
        "url": "https://github.com/Starrier/VS-Code-Plugin.git"
    },
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.helloWorld"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.helloWorld",
                "title": "Starrier"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "build": ""
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.17.50",
        "echarts": "^5.0.0",
        "eslint": "^5.16.0",
        "typescript": "^3.9.7",
        "vscode": "^1.1.37",
        "webpack": "^5.11.1",
        "webpack-cli": "^4.3.1"
    },
    "dependencies": {}
}
